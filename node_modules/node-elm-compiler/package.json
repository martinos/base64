  "dependencies": {
    "cross-spawn": "4.0.0",
    "find-elm-dependencies": "2.0.1",
    "lodash": "4.17.11",
    "temp": "^0.8.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.15",
    "chai": "3.5.0",
    "glob": "7.1.1",
